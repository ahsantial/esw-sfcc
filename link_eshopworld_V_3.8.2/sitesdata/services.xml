        <enabled>true</enabled>
        <log-prefix>ESWOrderReturnService</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <force-prd-enabled>true</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>ESWOrderReturnServiceProfile</profile-id>
        <credential-id>ESWOrderReturnServiceCredentials</credential-id>
